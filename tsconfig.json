{
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "declaration": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "lib": ["es2020",
      "dom"],
    "skipLibCheck": true,
    "downlevelIteration": true,
    "types": ["jest"],
    "outDir": "./dist"
  },
  "exclude": ["node_modules",
    "__tests__",
    "packages/stream/dist",
    "local"]
}